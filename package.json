{
	"name": "crbt-website",
	"version": "3.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "prisma generate && vite build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
