	},
	"devDependencies": {
		"@sveltejs/adapter-netlify": "^1.0.0-next.35",
		"@sveltejs/kit": "^1.0.0-next.405",
		"discord-api-types": "^0.37.10",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"prisma": "^3.10.0",
		"sass": "^1.45.2",
		"stream": "^0.0.2",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.4",
