	},
	"devDependencies": {
		"@purplet/polyfill": "^1.2.1",
		"@purplet/utils": "^1.0.0",
		"@sveltejs/adapter-netlify": "^1.0.0-next.35",
		"@sveltejs/kit": "^1.0.0-next.563",
		"@types/node": "^18.11.10",
		"discord-api-types": "^0.37.20",
		"dotenv": "^16.0.3",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "^4.7.1",
